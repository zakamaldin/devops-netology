---
all:
  hosts:
    %{~ for i in vms ~}
    ${i["name"]}-01:
      ansible_host: ${i["network_interface"][0]["nat_ip_address"]}  
  %{~ endfor ~}
  children:
    jenkins:
      children:
        jenkins_masters:
          hosts:
            jenkins-master-01:
        jenkins_agents:
          hosts:
              jenkins-agent-01:
  vars:
    ansible_connection_type: paramiko
    ansible_user: fedora